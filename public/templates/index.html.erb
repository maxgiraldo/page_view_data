<section>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1><a href="/top_urls">Top Urls</a>/<a href="/top_referrers">Top Referrers</a></h1>
            </div>
        </div>

        <div class="row" ng-controller="TopUrlCtrl">
            <div ng-repeat="(date, val) in top_urls">
                <div class="col-md-2">
                    <h3>{{ date }}</h3>
                    <div ng-repeat="result in val">
                        <p><strong>{{ result.url }}</strong> - {{ result.count }}</p>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>